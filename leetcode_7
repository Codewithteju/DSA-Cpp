#include<iostream>
#include<math.h>
using namespace std;
class Solution {
public:
    int reverse(int x) {
        int digit=0;
        int ans=0;
        while(x!=0){
            digit=x%10;
            if (ans >= -214748364 && ans <= 214748364) {
             ans = ans * 10 + digit;
            }
            else {
               ans = 0;
               break;
            }
            x=x/10;
        }
        return ans;
    }
};